{
  "type": "module",
  "name": "qwik-mikro-orm-template",
  "description": "Qwik template w/ Mikro ORM and MySQL",
  "engines": {
    "node": ">=18"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.node": "pnpm run build.client && vite build -c adapters/node/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.open": "vite --open --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview",
    "preview.open": "qwik build preview && vite preview --open",
    "serve.node": "pnpm build.node && cross-env PLATFORM_NAME=node node --env-file=.env.local server/entry.node.js",
    "start": "PLATFORM_NAME=node node server/entry.node.js",
    "preinstall": "npx only-allow pnpm"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/server/lib/db/cli.ts"
    ]
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.4.5",
    "@builder.io/qwik-city": "^1.4.5",
    "@mikro-orm/migrations": "^6.1.4",
    "@mikro-orm/seeder": "^6.1.4",
    "@octetstream/eslint-config": "^8.0.2",
    "@types/eslint": "^8.56.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.6",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-qwik": "^1.4.5",
    "postcss": "^8.4.31",
    "tailwindcss": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-typescript-transform": "^1.2.2",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@mikro-orm/cli": "^6.1.4",
    "@mikro-orm/core": "^6.1.4",
    "@mikro-orm/mysql": "^6.1.4",
    "@qwikest/icons": "^0.0.13",
    "date-fns": "^3.3.1",
    "lodash-es": "^4.17.21",
    "mysql2": "^3.9.1",
    "undici": "6.6.2",
    "zod": "^3.22.4"
  }
}
